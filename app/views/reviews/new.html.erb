<h1>New Review for <%= @book.title %></h1>
<%= form_for [@book, @review] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :user %>
  <%= f.text_field :user %>
  <br>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <br>
  <%= f.label :text %>
  <%= f.text_area :text %>
  <br>
  <%= f.submit "Submit Review" %>
<% end %>