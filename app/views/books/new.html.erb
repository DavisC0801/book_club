<h1>New Book</h1>
<%= form_for @book do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :year_published %>
  <%= f.text_field :year_published %>

  <%= f.label :page_count %>
  <%= f.text_field :page_count %>

  <%= f.label :thumbnail %>
  <%= f.text_field :thumbnail %>

  <%= f.label :authors %>
  <%= f.text_field :authors %>

  <%= f.submit %>
<% end %>
